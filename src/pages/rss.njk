{% extends "__layout.njk" %}

{% block Link %}
	<link rel="stylesheet" href="css/swiper.css">
{% endblock %}

{% block Header %}
	{% include "include/blue-line.njk" %}
{% endblock %}

{% block Content %}
	<section class="wrapper-blockadd wrapper-blockadd--no-offset">
		<div class="container">
			<div class="wrapper-blockadd__wrap">
				<picture>
					<source srcset="img/baadd2.jpg" type="image/jpeg" media="(max-width: 400px)">
					<img src="img/baadd1.jpg" alt="">
				</picture>
			</div>
		</div>
	</section>
	<div class="breadcrumbs">
		<div class="container">
			<ul class="breadcrumbs__list">
				<li class="breadcrumbs__item">
					<a href="" class="breadcrumbs__link">Главная</a>
					<svg>
						<use xlink:href="img/sprite.svg#svg--arrow-two">
					</svg>
				</li>
				<li class="breadcrumbs__item">
					<a href="" class="breadcrumbs__link">Пункт меню</a>
					<svg>
						<use xlink:href="img/sprite.svg#svg--arrow-two">
					</svg>
				</li>
